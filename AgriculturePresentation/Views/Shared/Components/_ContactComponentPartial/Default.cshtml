@model Address

@*
    Contact Component Partial
*@

<div class="contact-agile" id="contact">
	<div class="container">
		<h3 class="tittle">Bizimle iletişime geç</h3>
		<p class="heading-btm-w3ls">Bize mesaj atabilirsin</p>
		<div class="col-md-3 contact-left">
			<h4>Lokasyon</h4>
			<p>@Model.Description1</p>
			<p>@Model.Description2,</p>
			<p>@Model.Description3</p>
			<h4>Telefon</h4>
			<p>+@Model.PhoneNumber</p>
		</div>
		@await Html.PartialAsync("/Views/Default/ContactPartial.cshtml", Model)
	</div>
</div>

<div class="map-w3ls">
	@Html.Raw(Model.MapInfo)
</div>
